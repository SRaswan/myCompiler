(if (isbool input)
    false
    (if (< input 2)
        false
        (let
            ((divisor 2))
            (loop
                (if (> (* divisor divisor) input)
                    (break true)
                    (if (let
                            ((quotient 1))
                            (loop
                                (if (> (* divisor quotient) input)
                                    (break false)
                                    (if (= (* divisor quotient) input)
                                        (break true)
                                        (set! quotient (+ quotient 1))))))
                        (break false)
                        (set! divisor (+ divisor 1))))))))
                        