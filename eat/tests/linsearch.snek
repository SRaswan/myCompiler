(fun (search lst key size)
    (let ((idx 0))
(loop
         (if (>= idx size)
     (break false)
         (if (= (vec-get lst idx) key)
         (break idx)
         (set! idx (add1 idx))
         )
         )
              
    ) 
    )
)
